{
    "rules": {
        "no-var-requires": false,
        "no-string-throw": true,
        "no-unused-expression": true,
        "no-duplicate-variable": true,
        "curly": true,
        "class-name": true,
        "semicolon": [
            true,
            "always"
        ],
        "triple-equals": true,
        "quotemark": [
            true,
            "double"
        ],
        "indent": [true, "spaces", 4]
    },
    "defaultSeverity": "warning"
}