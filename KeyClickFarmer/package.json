{
    "name": "keyclick-farmer",
    "displayName": "KeyClick Farmer",
    "description": "This is the game of using key-typing counts.",
    "version": "1.2.4",
    "publisher": "iyokarof",
    "icon": "images/icon-large.png",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/kato-hiroto/KeyClickFarmer.git"
    },
    "engines": {
        "vscode": "^1.28.0"
    },
    "categories": [
        "Themes",
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.keyclickfarmer-powerup",
                "title": "KeyClickFarmer-PowerUp"
            }
        ],
		"colors": [
			{
				"id": "extension.unit1BackGround",
				"description": "Background decoration color for Unit1 bonus.",
				"defaults": {
					"dark": "#FF000055",
					"light": "#FF000055",
					"highContrast": "#FF000055"
				}
			}
		]
	},
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test:default": "npm run compile && node ./node_modules/vscode/bin/test",
        "test": "tsc & mocha"
    },
    "devDependencies": {
        "@types/chai": "^4.2.5",
        "@types/mocha": "^2.2.42",
        "@types/node": "^8.10.59",
        "chai": "^4.2.0",
        "tslint": "^5.8.0",
        "typemoq": "^2.1.0",
        "typescript": "^2.9.2",
        "vscode": "^1.1.21"
    },
    "dependencies": {}
}
